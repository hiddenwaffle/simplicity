{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "quotemark": false,
        "member-access": false,
        "no-angle-bracket-type-assertion": false,
        "no-console": {
            "severity": "warning"
        },
        "variable-name": false,
        "trailing-comma": false,
        "ordered-imports": false,
        "interface-name": false,
        "max-classes-per-file": false
    },
    "rulesDirectory": []
}